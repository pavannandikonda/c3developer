@db(datastore='cassandra', partitionKeyField='merchant', persistenceOrder='transactionDate', compactType=true)
entity type MerchantTransaction schema name 'MERTRAN' {
  
  /**
   * Merchant associated with the transaction.
   */
  merchant: Merchant

  transactionDate: datetime
  amount:          ExactDimension
  transactionId:   string
}
